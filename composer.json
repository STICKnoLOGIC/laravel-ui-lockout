{
    "name": "sticknologic/laravel-ui-lockout",
    "description": "A Laravel package providing a customizable UI lockout component",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ui",
        "lockout",
        "blade",
        "component"
    ],
    "authors": [
        {
            "name": "John Aerial J. Azcune",
            "email": "johnaerial.azcune@sticknologic.is-a.dev",
            "role": "Developer",
            "homepage": "https://sticknologic.is-a.dev"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Snl\\LaravelUiLockout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snl\\LaravelUiLockout\\Tests\\": "tests/"
        }
    },      
    "extra": {
        "laravel": {
            "providers": [
                "Snl\\LaravelUiLockout\\LaravelUiLockoutServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
